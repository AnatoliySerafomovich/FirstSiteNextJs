generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  password   String
  dateCreate DateTime @updatedAt
  confirm    Boolean
  wallet     Int      @default(0)
  favoritId  Int[]
  basket     Int[]
}

enum Role {
  User
  Admin
}
